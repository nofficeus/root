{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "npx mix",
        "watch": "npx mix watch",
        "watch-poll": "npx mix watch -- --watch-options-poll=1000",
        "hot": "npx mix watch --hot",
        "hot-poll": "npx mix watch --hot -- --watch-options-poll=1000",
        "prod": "npm run production",
        "production": "npx mix --production",
        "ide": "php artisan ide-helper:generate && php artisan ide-helper:meta && php artisan ide-helper:models -N",
        "intl-extract": "formatjs extract 'resources/js/**/*.{js,jsx}' --format=transifex --out-file=resources/messages/en.json --id-interpolation-pattern=[sha512:contenthash:20]",
        "intl-compile": "formatjs compile resources/messages/en.json --format=transifex --out-file=resources/lang/en.json",
        "intl-compile-folder": "formatjs compile-folder resources/messages/ resources/lang/ --format=transifex",
        "intl": "npm run intl-extract && npm run intl-compile",
        "prepare": "husky install"
    },
    "lint-staged": {
        "resources/**/*.{js,jsx}": [
            "eslint --cache --fix"
        ]
    },
    "browserslist": [
        "> 5%, not dead"
